{
  "dependencies": {
    "main.js": {
      "files": [
        "scripts/main.js"
      ],
      "main": true
    },
    "head-inline.js": {
      "bower": ["loadcss", "fontfaceobserver"],
      "files": [
        "scripts/head-inline.js"
      ]
    },
    "main.css": {
      "files": [
        "styles/main.scss"
      ],
      "main": true
    },
    "head-inline.css": {
      "files": [
        "styles/head-inline.scss"
      ]
    },
    "login.css": {
      "files": [
        "styles/login.scss"
      ]
    },
    "jquery.js": {
      "bower": ["jquery", "jquery-migrate-official"]
    },
    "head.js": {
      "bower": ["pace", "lazysizes", "lazysizes-bgset", "lazysizes-respimg"]
    }
  },
  "config": {
    "devUrl": "http://localhost/x/public_html",
    "criticalCssFileName": "head-critical",
    "criticalCssUrls": [
      "http://localhost/x/public_html"
    ],
    "criticalCssSizes": [
      {
        "width": 320,
        "height": 568
      },
      {
        "width": 720,
        "height": 1024
      },
      {
        "width": 1366,
        "height": 700 
      },
      {
        "width": 1920,
        "height": 1000
      }
    ],
    "forceIncludeSelectors": [
      ".lazyload",
      ".lazyloading",
      ".lazyloaded",
      ".sticky",
      ".relevanssi-query-term",
      ".wf-loading",
      ".wf-active",
      ".wf-inactive",
      ".is-active"
    ],
    "gFonts": ""
  }
}
