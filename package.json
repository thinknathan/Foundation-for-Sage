{
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=development run-s mix",
    "build:production": "cross-env NODE_ENV=production run-s clean mix",
    "start": "cross-env NODE_ENV=development run-s \"mix --watch\"",
    "hot": "cross-env NODE_ENV=development run-s build mix:hot",
    "mix": "webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix:hot": "webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "lint": "run-s -c lint:*",
    "lint:scripts": "eslint assets/scripts",
    "lint:styles": "stylelint \"assets/**/*.{vue,css,sass,scss,less}\"",
    "test": "run-s -c lint"
  },
  "devDependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "browser-sync": "^2.3",
    "browser-sync-webpack-plugin": "2.3",
    "cross-env": "^7.0",
    "eslint": "^7.28",
    "eslint-plugin-import": "^2.23",
    "laravel-mix": "^6.0",
    "laravel-mix-copy-watched": "^2.2.1",
    "npm-run-all": "^4.1",
    "postcss": "^8.2",
    "resolve-url-loader": "3.1",
    "rimraf": "^3.0",
    "sass": "^1.32",
    "sass-loader": "11.*",
    "stylelint": "^13.13",
    "stylelint-config-standard": "^22.0",
    "vue-template-compiler": "^2.6"
  },
  "dependencies": {
    "aos": "~2",
    "foundation-sites": "^6.6.3",
    "fr-accordion": "^1.0.6",
    "fr-dialogmodal": "^1.0.4",
    "fr-offcanvas": "^1.1.1",
    "fr-tabs": "^1.0.6",
    "glider-js": "^1.7.7",
    "gravity-forms-scss": "git+https://git@github.com/wpsitecare/gravity-forms-scss.git",
    "headroom.js": "^0.12.0",
    "idle-until-urgent": "^1.0.0",
    "js-cookie": "^2.2.1",
    "metismenujs": "^1.0.3",
    "pikaday": "^1.8.0",
    "rellax": "^1.12.1",
    "sass-divider": "git+https://git@github.com/thinknathan/sass-divider.git",
    "sass-hover-transition": "git+https://git@github.com/thinknathan/sass-hover-transition.git",
    "sass-inline-icons": "git+https://git@github.com/thinknathan/sass-inline-icons.git",
    "sass-ripple": "git+https://git@github.com/thinknathan/sass-ripple.git",
    "scroll-watcher": "~2",
    "stickybits": "^3.7.9",
    "tobii": "^2.0.0-alpha"
  }
}
